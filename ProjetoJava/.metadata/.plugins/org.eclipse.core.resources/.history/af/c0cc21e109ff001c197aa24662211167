
public abstract class Conta implements IConta { /*essa classe foi definida como abstract, para que só as filhas possam instanciá-la.*/
	
	protected static final int AGENCIA_PADRAO = 1; /*AGENCIA_PADRÃO é uma constante que foi criada aqui, para que possa ser acessada tanto na classe
	ContaCorrente, quanto na classe ContaPoupança*/
	
	protected int agencia;
	protected int numero;
	protected double saldo;
	
	
	public void sacar(double valor) {
		
	}

	public void depositar(double valor) {
		
	}

	public void transferir(double valor, Conta contaDestino) {
		
	}
	
	
	public int getAgencia() {
		return agencia;
	}

	public int getNumero() {
		return numero;
	}

	public double getSaldo() {
		return saldo;
	}

	
	

}
